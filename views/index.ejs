                                        <p id="available">
                                            rahman.sisdis
                                            <i class="material-icons right">done</i>
                                        </p>
                                    </div>
                                    <% }else{ %>
                                    <div class="card-panel red lighten-2 col s4 push-s3">
                                        <p id="available">
                                            rahman.sisdis
                                            <i class="material-icons right">clear</i>
